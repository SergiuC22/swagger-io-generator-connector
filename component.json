{
    "title": "Swagger Generator",
    "description": "This is an online swagger codegen server.  You can find out more at https://github.com/swagger-api/swagger-codegen or on [irc.freenode.net, #swagger](http://swagger.io/irc/).",
    "docsUrl": "",
    "url": "https://api.apis.guru/v2/specs/swagger.io/generator/2.4.2/swagger.json",
    "envVars": {},
    "credentials": {
        "fields": {
            "server": {
                "label": "Server",
                "viewClass": "SelectView",
                "model": [
                    "https://generator.swagger.io/api",
                    "--- Custom URL"
                ],
                "required": true
            },
            "otherServer": {
                "label": "Custom Server URL",
                "viewClass": "TextFieldView"
            }
        }
    },
    "triggers": {
        "startFlow": {
            "main": "./lib/triggers/startFlow.js",
            "type": "polling",
            "title": "Start Flow",
            "fields": {}
        }
    },
    "actions": {
        "clientOptions": {
            "main": "./lib/actions/clientOptions.js",
            "title": "Gets languages supported by the client generator",
            "description": "",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/clientOptions.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "getClientOptions": {
            "main": "./lib/actions/getClientOptions.js",
            "title": "Returns options for a client library",
            "description": "",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/getClientOptions.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "generateClient": {
            "main": "./lib/actions/generateClient.js",
            "title": "Generates a client library",
            "description": "Accepts a `GeneratorInput` options map for spec location and generation options",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/generateClient.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "downloadFile": {
            "main": "./lib/actions/downloadFile.js",
            "title": "Downloads a pre-generated file",
            "description": "A valid `fileId` is generated by the `/clients/{language}` or `/servers/{language}` POST operations.  The fileId code can be used just once, after which a new `fileId` will need to be requested.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/downloadFile.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "serverOptions": {
            "main": "./lib/actions/serverOptions.js",
            "title": "Gets languages supported by the server generator",
            "description": "",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/serverOptions.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "getServerOptions": {
            "main": "./lib/actions/getServerOptions.js",
            "title": "Returns options for a server framework",
            "description": "",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/getServerOptions.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "generateServerForLanguage": {
            "main": "./lib/actions/generateServerForLanguage.js",
            "title": "Generates a server library",
            "description": "Accepts a `GeneratorInput` options map for spec location and generation options.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/generateServerForLanguage.in.json",
                "out": {
                    "type": "object"
                }
            }
        }
    }
}